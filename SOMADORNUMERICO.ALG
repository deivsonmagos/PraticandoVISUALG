Algoritmo "Conta1a10"

Var

      Cont, N, R : Inteiro

Inicio
      cont <- 1
     Escreva("Quer ver a tabuada de qual nume: ? ")
     Leia(N)
      Repita
            R <- N * Cont
          escreval(N, " X ", Cont, " = ", R)
        Cont <- cont + 1
      
      Ate(Cont > 10)


Fimalgoritmo