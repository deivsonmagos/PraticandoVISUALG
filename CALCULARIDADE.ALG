Algoritmo "CalcularIdade"

Var
    ano, idade, nasc: Inteiro


Inicio
     Escreva("Em que ano estamos? ")
     Leia(ano)
     Escreva("Em que ano voçê nasceu? ")
     Leia(nasc)
     idade <- ano - nasc
     Escreva("Em", ano, " Voçê tera ", idade, " Anos, ")
     se (idade >= 21) entao
        Escreva("e voçê já, tera atingido a maioridade. ")

        fimSe


Fimalgoritmo