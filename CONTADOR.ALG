Algoritmo "Contador1-0"

Var
        valor,contador, salto : inteiro


Inicio
   contador <- 0
   Escreva("Quer contar ate quanto ? ")
   Leia(valor)
   Escreva("Qual vai ser o valor do salto ? ")
   Leia(salto)
   Enquanto (contador <= valor ) faca
     Escreval(contador)
     contador <- contador + salto
     
   
   FimEnquanto
   Escreval("Terminei de contar")
Fimalgoritmo