Algoritmo "Creusa1"

Var
     id, nasc, anoAtual :inteiro


Inicio
       Escreval("Digite o ano atual:")
       Leia(anoAtual)
       Escreval("Digite a data de seu nascimento:")
       Leia(nasc)
       id <- anoAtual - nasc
       Escreval("Voçê tem ", id, " anos de idade!")


Fimalgoritmo